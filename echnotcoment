        <script>

    $(document).ready(function(){
    var commentElements = $('.diary__comment__new');
    var commentIds = [];
    commentElements.each(function() {
      var commentId = $(this).data('commentid');
      if (commentId) {
        commentIds.push(commentId);
      }
    });

    if ((commentIds === undefined || commentIds.length == 0) == false) {
        $.ajax({
            url: '/users/diaries/confirmhomeworkcomment',
            data: {
                rooId: '32',
                instituteId: '1189',
                departmentId: '297176',
                pupilId: '1435854',
                commentId: commentIds
            },
            type: 'POST',
            dataType: "json",
            success: function (data) {
            },
            error: function (data) {
            }
        });
    }
    });

    var PERFECT_URL = /((https?: \/\/(?:www\.|(?!www))[a-zA-Z0-9][a-zA-Z0-9]+[a-zA-Z0-9]\.[^\s]{2,}|www\.[a-zA-Z0-9][a-zA-Z0-9]+[a-zA-Z0-9]\.[^\s]{2,}|https?:\/\/(?:www\.|(?!www))[a-zA-Z0-9\-]+\.[^\s]{2,}|www\.[a-zA-Z0-9]+\.[^\s]{2,}))/ig;

    $(document).ready(function () {
        $('.navigation__years__department').text($('.navigation__years_selected').text());
        if (localStorage.getItem('diary_dark_theme_1435855') == 'true') {
            $('.diary__theme').find('.fa').removeClass('fa-moon-o').addClass('fa-sun-o');
        }
        $('.navigation__datepicker').datepicker('destroy').datepicker({
            format: 'dd.mm.yyyy',
            todayBtn: "linked",
            startDate: '01.09.2024',
            endDate: '31.05.2025',
            weekStart: 1,
            language: "ru",
            calendarWeeks: true,
            todayHighlight: true,
            clearBtn: true,
            autoclose: true,
            disableTouchKeyboard: true
        });
        $('.diary__homework-text, .diaries-mobile__homework-text, .diary__materials').each(function () {
            $(this).html(findLinks($(this).html()));
        });
        resize();
        $('.diaries .diary__mark').each(function () {
            $(this).popover({
                content: $(this).data('popover-content'),
                container: $('.diaries'),
                trigger: 'hover',
                delay: 0,
                html: true,
                placement: 'top'
            });
        });
        $('.diaries_mobile .diary__mark').each(function () {
            $(this).popover({
                content: $(this).data('popover-content'),
                container: $(this),
                trigger: 'manual',
                delay: 0,
                html: true,
                placement: 'top'
            });
        });
        $('.navigation [data-popover-content]').each(function () {
            $(this).popover({
                content: $(this).data('popover-content'),
                container: $('.navigation'),
                trigger: 'hover',
                delay: 0,
                html: true,
                placement: 'bottom'
            });
        });
        $('.diaries [data-popover-content]:not(.diary__mark)').each(function () {
            $(this).popover({
                content: $(this).data('popover-content'),
                container: $('.diaries'),
                trigger: 'hover',
                delay: 0,
                html: true,
                placement: 'bottom'
            });
        });
        setTimeout(recalc_mobile_content, 50);
    });

    function findLinks(content) {
        var _content = content.replace(PERFECT_URL, '<a class="diary__link" target="_blank" href="$1">$1</a>');
        return _content;
    }

    $(window).on('resize', resize);

    function resize() {
        $('tr:last-child .diary__discipline, tr:last-child .diary__nolesson').height('');
        if (window.innerWidth > 991) {
            $('.diaries > div:first-child tbody').each(function () {
                var index = $(this).index() - 1;
                var second_tbody = $('.diaries > div:nth-child(2) tbody').eq(index);
                var height_1 = $(this).outerHeight();
                var height_2 = second_tbody.outerHeight();
                if (height_1 > height_2) {
                    var inter_height_td = second_tbody.find('tr:last-child .diary__discipline, tr:last-child .diary__nolesson');
                    inter_height_td.outerHeight(height_1 - height_2 + inter_height_td.outerHeight());
                } else {
                    var inter_height_td = $(this).find('tr:last-child .diary__discipline, tr:last-child .diary__nolesson');
                    inter_height_td.outerHeight(height_2 - height_1 + inter_height_td.outerHeight());
                }
            });
        }
    }

    function recalc_mobile_content() {
        $('.diaries_mobile__day').each(function () {
            var unseen_marks_count = $(this).find('.diary__mark-not-seen-before').length;
            if (unseen_marks_count > 0) {
                $(this).find('.diaries_mobile__day__unseenmarks').html(unseen_marks_count_declension(unseen_marks_count));
            } else {
                $(this).find('.diaries_mobile__day__unseenmarks').hide();
            }
            var comment_count = $(this).find('.diaries_mobile__lesson__commentteacher').length;
            if (comment_count > 0) {
                $(this).find('.diaries_mobile__day__commentcount').html(comment_count_declension(comment_count))
            } else {
                $(this).find('.diaries_mobile__day__commentcount').hide();
            }
            var late_sum = 0;
            $(this).find('.diaries_mobile__lesson__late').each(function () {
                late_sum += $(this).data('late-min');
            });
            if (late_sum > 0) {
                $(this).find('.diaries_mobile__day__latesumm').html('<i class="fa fa-clock-o"></i>&nbsp;' + late_sum + ' мин')
            } else {
                $(this).find('.diaries_mobile__day__latesumm').hide();
            }
            $(this).find('.diaries_mobile__day__lessonscount').text(lessonscount_declension(parseInt($(this).find('.diaries_mobile__day__lessonscount').text())));
        });
    }

    function unseen_marks_count_declension(number) {
        var text = '';
        if ((number > 4 && number < 21) || number % 10 == 0) {
            text = number + ' новых оценок';
        } else if (number % 10 == 1) {
            text = number + ' новая оценка';
        } else if (number % 10 < 5) {
            text = number + ' новые оценки';
        } else {
            text = number + ' новых оценок';
        }
        return text;
    }

    function comment_count_declension(number) {
        var text = '';
        if ((number > 4 && number < 21) || number % 10 == 0) {
            text = number + ' комментариев';
        } else if (number % 10 == 1) {
            text = number + ' комментарий';
        } else if (number % 10 < 5) {
            text = number + ' комментария';
        } else {
            text = number + ' комментариев';
        }
        return text;
    }

    function lessonscount_declension(number) {
        var text = '';
        if ((number > 4 && number < 21) || number % 10 == 0) {
            text = number + ' уроков';
        } else if (number % 10 == 1) {
            text = number + ' урок';
        } else if (number % 10 < 5) {
            text = number + ' урока';
        } else {
            text = number + ' уроков';
        }
        return text;
    }

    $(document).off('click.open_day').on('click.open_day', '.diaries_mobile__day:not(.diaries_mobile__day-opened)', function () {
        open_day(this);
    });

    function open_day(self) {
        $(document).off('click.open_day');
        $(self).addClass('diaries_mobile__day-opened');
        $(self).closest('.diaries_mobile__wrapper').addClass('diaries_mobile__wrapper-opened');
        $(self).closest('.diaries_mobile').addClass('diaries_mobile-opened');
        setTimeout(function () {
            $(document).on('click.close_day', '.diaries_mobile__day-opened .diaries_mobile__day__collapser', function () {
                close_day(this);
            });
        }, 1);
        $('html, body').animate({ 'scrollTop': 0 }, 250);
    }

    function close_day(self) {
        $(document).off('click.close_day');
        $(self).closest('.diaries_mobile__day').removeClass('diaries_mobile__day-opened');
        $(self).closest('.diaries_mobile__wrapper').removeClass('diaries_mobile__wrapper-opened');
        $(self).closest('.diaries_mobile').removeClass('diaries_mobile-opened');
        setTimeout(function () {
            $(document).on('click.open_day', '.diaries_mobile__day:not(.diaries_mobile__day-opened)', function () {
                open_day(this);
            });
        }, 1);
    }

    // "Просмотреть" оценку
    $('.diary__mark-not-seen-before').click(function () {
        AjaxConfirmSingleMark(this);
        var mark_id = $(this).attr('model-mark-id');
        var this_marks = $('.diary__mark-not-seen-before[model-mark-id="' + mark_id + '"]');
        this_marks.addClass('diary__mark-just-seen');
        // пересчёт количества новых оценок на мобилке
        var day_unseen_counter_block = $('.diaries_mobile .diary__mark-not-seen-before[model-mark-id="' + mark_id + '"]').closest('.diaries_mobile__day').find('.diaries_mobile__day__unseenmarks');
        var day_unseen_counter = parseInt(day_unseen_counter_block.text());
        if (day_unseen_counter > 1) {
            day_unseen_counter_block.text(unseen_marks_count_declension(day_unseen_counter - 1));
        } else {
            day_unseen_counter_block.text('').addClass('d-none');
        }
        setTimeout(function () {
            var date_popover = this_marks.eq(0).attr('model-mark-date');
            this_marks.data('popover-content', 'Дата проставления оценки: ' + date_popover);
            this_marks.popover('dispose');
            this_marks.each(function () {
                if (!$(this).closest('.diaries_mobile').length) {
                    $(this).popover({
                        content: $(this).data('popover-content'),
                        container: $(this),
                        trigger: 'hover',
                        delay: 0,
                        html: true,
                        placement: 'top'
                    });
                }
            });
            this_marks.removeClass('diary__mark-not-seen-before').removeClass('diary__mark-just-seen');
        }, 250);

        //Подсчёт количества непросмотренных оценок
        var number_of_unseen = $('.diaries .diary__mark-not-seen-before:not(.diary__mark-just-seen)').length;
        $('.parent_signature__marks_counter').html(number_of_unseen);
        // проверить, вдруг все галочки за неделю кликнуты, - тогда скрыть кнопку "Просмотреть все оценки"
        if (number_of_unseen == 0) {
            parent_signature_seeing();
        }
    });

    // клик по "Просмотреть все оценки" на десктопе, чтобы "просмотреть" все оценки
    $('.parent_signature__btn').click(function () {
        AjaxConfirmAllMarks();
        $('.diary__mark-not-seen-before').addClass('diary__mark-just-seen');
        $('.diaries_mobile__day__unseenmarks').text('').addClass('d-none');
        parent_signature_seeing();
        setTimeout(function () {
            $('.diary__mark-just-seen').each(function () {
                if (!$(this).closest('.diaries_mobile').length) {
                    $(this).data('popover-content', 'Дата проставления оценки: ' + $(this).attr('model-mark-date')).popover('dispose');
                    $(this).popover({
                        content: $(this).data('popover-content'),
                        container: $(this),
                        trigger: 'hover',
                        delay: 0,
                        html: true,
                        placement: 'top'
                    });
                }
            });
            $('.diary__mark-just-seen').removeClass('diary__mark-just-seen').removeClass('diary__mark-not-seen-before');
        }, 250);
    });

    function parent_signature_seeing() {
        $('.parent_signature__btn').addClass('d-none');
        $('.parent_signature__seen').removeClass('d-none');
        var self = $('.parent_signature__seen');
        var text = self.text();
        var counter = 0;
        self.html('&nbsp;');
        var timer = setInterval(function () {
            counter++;
            self.text(text.substr(0, counter));
        }, 100);
        setTimeout(function () {
            clearInterval(timer);
        }, 100 * (text.length + 1));
    }

    function AjaxConfirmSingleMark(cell) {
        $.ajax({
            url: '/users/diaries/confirmregularmark',
            data: {
                rooId: '32',
                instituteId: '1189',
                departmentId: '297176',
                pupilId: '1435854',
                id: $(cell).attr('model-mark-id'),
                absence: $(cell).attr('model-type') == 'absence' ? true : false
            },
            type: 'get',
            dataType: "json",
            success: function (data) {
            },
            error: function (data) {
                addSystemMessage({ type: 'danger', text: 'Произошла ошибка, пожалуйста, попробуйте позже.' });
            }
        });
    };

    function AjaxConfirmAllMarks() {
        $.ajax({
            url: '/users/diaries/confirmregularmarksbydaterange',
            data: {
                rooId: '32',
                instituteId: '1189',
                departmentId: '297176',
                pupilId: '1435854',
                startDate: '2024-10-14',
                endDate: '2024-10-20'
            },
            type: 'get',
            dataType: "json",
            success: function (data) {
            },
            error: function (data) {
                addSystemMessage({ type: 'danger', text: 'Произошла ошибка, пожалуйста, попробуйте позже.' });
            }
        });
    };

    $('.diary__theme').on('click', function () {
        $('.diary__theme').find('.fa').toggleClass('fa-sun-o').toggleClass('fa-moon-o');
        if ($('.diary__theme .fa-sun-o').length) {
            switch_theme('dark');
            localStorage.setItem('diary_dark_theme_1435855', 'true');
        } else {
            switch_theme('default');
            localStorage.setItem('diary_dark_theme_1435855', 'false');
        }
    });

    $('.navigation__years .dropdown-item').on('click', function () {
        waitingDialog.show();
        location.href = '/users/diaries/details'
            + '?rooId=32'
            + '&instituteId=' + $(this).attr('model-institute-id')
            + '&departmentId=' + $(this).attr('model-department-id')
            + '&pupilId=1435854'
            + '&year=' + $(this).attr('model-start-year')
            + '&log=false';
    });

    $('.navigation__datepicker').on('change', function () {
        var d = new Date($(this).datepicker('getDate'));
        if (d.getDay() > 0) {
            d.setDate(d.getDate() - d.getDay() + 1);
        } else {
            d.setDate(d.getDate() - 6);
        }
        var dd = new Date(d);
        dd.setMonth(0, 1);
        var week = Math.ceil((((d - dd) / 86400000) + 1) / 7) + 1;
        if (week == 1 && dd.getDay() != 1) {
            d = new Date(d.getFullYear() - 1, 11, 31);
            dd = new Date(d.getFullYear(), 11, 31);
            dd.setMonth(0, 1);
            week = Math.ceil((((d - dd) / 86400000) + 1) / 7);
        }
        window.location.href = '/users/diaries/details'
            + '?rooId=32'
            + '&instituteId=1189'
            + '&departmentId=297176'
            + '&pupilId=1435854'
            + '&year=' + d.getFullYear()
            + '&week=' + week
            + '&log=false';
    });

    $('.diaries_mobile .diary__mark').on('click', function () {
        if ($(this).attr('aria-describedby') > 0) {
            $(this).popover('hide');
        } else {
            $('.diaries_mobile .diary__mark[aria-describedby]').popover('hide');
            $(this).popover('show');
            $(document).on('click.popover_miss', function (e) {
                if ($(e.target).closest('.diary__mark').length) {
                    return false;
                }
                $(document).off('click.popover_miss');
                $('.diaries_mobile .diary__mark[aria-describedby]').popover('hide');
            });
        }
    });

    $(document).off('click.delete_homework').on('click.delete_homework', '.diary__homework__pupil-file__delete', function (e) {
        e.preventDefault();
        var self = $(this);
        if (!confirm('Вы уверены, что хотите удалить этот файл?')) {
            return false;
        }
        $.ajax({
            url: '/users/homework/delete/' + $(self).data('file-id'),
            type: 'POST',
            success: function (response) {
                if (response.Status == 'success') {
                    addSystemMessage({ type: 'success', text: response.Message });
                    $(self).closest('.diary__homework__pupil-file__wrapper').remove();
                    resize();
                } else {
                    addSystemMessage({ type: 'danger', text: response.Message });
                }
            },
            error: function () {
                addSystemMessage({ type: 'danger', text: 'Произошла ошибка, пожалуйста, попробуйте позже' });
            }
        })
    });

    $('.navigation__datepicker').datepicker().on('show', function (e) {
        $('.datepicker .clear').html('<i class="fa fa-times"></i>');
        $('.datepicker .clear').one('click', function () {
            $('.navigation__datepicker').datepicker('hide');
        });
    });

    $('.diaries_mobile__lesson__teacherchat, .diary__chat-teacher, .diaries_mobile__lesson__commentteacher .create-modal').on('click', function () {
        var lesson_date = $(this).data('lesson-date');
        $(document).one('shown.bs.modal', '[id^="chat-modal"]', function (e) {
            $('.modal textarea').val('Здравствуйте. Вопрос насчёт урока от ' + lesson_date + ': ');
        });
    });

    $('.diaries__videoconf').on('click', function (e) {
        e.preventDefault();
        var webinarId = $(this).attr('data-webinar-id');

        waitingDialog.show();

        var mainDef = $.Deferred().done(waitingDialog.hide);
        var apiRequestDef = $.Deferred();
        var apiUrl = '/webinar/' + webinarId;

        $.post(apiUrl)
            .done(function (response) {
                switch (response.Status) {
                    case 'success':
                        apiRequestDef.resolve(response.Data);
                        break;
                    case 'danger':
                        apiRequestDef.reject(response.Message);
                        break;
                    default:
                        apiRequestDef.reject('Произошла ошибка');
                        break;
                }
            })
            .fail(function () {
                apiRequestDef.reject('Произошла ошибка');
            });

        apiRequestDef
            .done(function (data) {
                var newWindow = open(data.createUrl);
                setTimeout(function () {
                    newWindow.location.href = data.connectUrl;
                }, 1000);
            })
            .fail(function (message) {
                systemMessage.add({ type: 'danger', text: message });
            })
            .always(function () {
                mainDef.resolve();
            });
    });
        </script>
    </main>

